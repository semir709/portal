
<div class="container ps-5 ps-sm-4">

    <%-include('partials/top_header_admin.ejs') %>

    <div class="row ms-3 mt-4">

        <div class="col-xl-3 col-lg-4 col-md-5 mb-3 ss_img_view">
    
            <div class="ss_img_holder">
    
                <img src="<%= data[0].image %>" alt="">
    
            </div>
    
        </div>
    
        <div class="col-xl-1 col-lg-1 col-md-1"></div>
    
        <div class="col-xl-8 col-lg-7 col-md-6 ss_user_info_view">
    
            <div class="d-flex ss_social_container_view">
    
                <span><i class="fab fa-facebook-square"></i></span>
                <p><%= data[0].facebook %></p>
            </div>
    
            <div class="d-flex ss_social_container_view">
                <span><i class="fab fa-instagram-square"></i></span>
                <p><%= data[0].instagram %></p>
            </div>
    
            <div class="d-flex ss_social_container_view">
                <span><i class="fab fa-twitter-square"></i></span>
                <p><%= data[0].twitter %></p>
            </div>
    
        </div>
    
    </div>
    
    <div class="row ms-3 ">
        <div class="col-12">
    
            <p><%= data[0].e_mail %></p>
            <p><%= data[0].num %></p>
    
            <div class="ss_user_info_view">
    
                <%= data[0].about %>
    
            </div>
    
    
        </div>
    
    
    </div>


</div>